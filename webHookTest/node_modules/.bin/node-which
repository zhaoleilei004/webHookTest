#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node --max-old-space-size=4048"  "$basedir/../which/bin/node-which" "$@"
  ret=$?
else 
  node --max-old-space-size=4048  "$basedir/../which/bin/node-which" "$@"
  ret=$?
fi
exit $ret

